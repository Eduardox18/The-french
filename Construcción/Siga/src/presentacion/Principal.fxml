<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<MenuBar xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentacion.ControladorPrincipal">
  <menus>
    <Menu mnemonicParsing="false" text="Administrar">
      <items>
        <MenuItem mnemonicParsing="false" text="Actividades" />
            <Menu mnemonicParsing="false" text="Reservacion">
              <items>
                <MenuItem mnemonicParsing="false" text="Conversación" />
                  <MenuItem mnemonicParsing="false" text="Asesoría" />
              </items>
            </Menu>
            <MenuItem mnemonicParsing="false" text="Lista de Asistencia" />
            <MenuItem mnemonicParsing="false" text="Seguimiento del Alumno" />
            <MenuItem mnemonicParsing="false" text="Experiencias Educativas" />
      </items>
    </Menu>
    <Menu mnemonicParsing="false" text="Reservar">
      <items>
        <MenuItem fx:id="barReservarActividad" mnemonicParsing="false" onAction="#abrirReservarActividad" text="Actividad" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Registrar">
        <items>
          <MenuItem mnemonicParsing="false" text="Hoja de Seguimiento" />
            <MenuItem fx:id="barRegistrarBitacora" mnemonicParsing="false" onAction="#abrirRegistrarBitacora" text="Bitácora" />
            <MenuItem mnemonicParsing="false" text="Autoevaluación" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Consultar">
        <items>
          <MenuItem mnemonicParsing="false" text="Grupos" />
            <MenuItem mnemonicParsing="false" text="Avances" />
            <MenuItem mnemonicParsing="false" text="Historial Avisos" />
            <MenuItem mnemonicParsing="false" text="Resultados Evidencias" />
            <Menu mnemonicParsing="false" text="Portafolio de Evidencias">
              <items>
                <MenuItem mnemonicParsing="false" text="Autoevaluación" />
                  <MenuItem mnemonicParsing="false" text="Actividades" />
                  <MenuItem mnemonicParsing="false" text="Hoja de Seguimiento" />
                  <MenuItem mnemonicParsing="false" text="Bitácora" />
              </items>
            </Menu>
            <MenuItem mnemonicParsing="false" text="Calendario de Actividades" />
            <MenuItem mnemonicParsing="false" text="Historial de Observaciones" />
        </items>
      </Menu>
  </menus>
</MenuBar>
